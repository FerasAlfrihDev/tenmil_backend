{
    "version": 2,
    "builds": [
        {
        "src": "build_files.sh",
        "use": "@vercel/static"
        },
        {
            "src": "configurations/wsgi.py",
            "use": "@vercel/python"
            
        }
    ],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "configurations/wsgi.py"
        }
    ]
}