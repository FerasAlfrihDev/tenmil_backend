{
    "version": 2,
    "builds": [
        {
            "src": "configurations/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": { 
                "maxLambdaSize": "15mb",
                "pythonVersion": "3.12.6" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "configurations/wsgi.py"
        }
    ]
}